{
  "manifest_version": 2,
  "name": "No Tweet Views",
  "version": "1.0", 
  "author": "Gameleon",
  "description": "Removes the views indicator from tweets",
  "icons": {
    "48": "icons/icon48.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/jquery-3.6.3.min.js", "index.js"],
      "matches": [
        "https://twitter.com/*",
        "https://mobile.twitter.com/*",
        "https://m.twitter.com/*"
      ]
    }
  ]
}
